@model List<DevFolio.Models.TblProject>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Projeler sayfası</h2>
<table class="table table-bordered">
    <tr>
        <th> #   </th>
        <th> Proje Başlığı   </th>
        <th> Kategori   </th>
        <th> Githup'da Aç   </th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.ProjectID</td>
        <td>@item.Title</td>
        <td>@item.TblCategory.CategoryName</td>
        <td><a href="#">GithuB'da Aç</a></td>
    </tr>
    }
    
  
</table>

